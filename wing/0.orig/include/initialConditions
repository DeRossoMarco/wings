velocity            8;
alpha               25;
U                   #eval "$velocity*cos(degToRad($alpha))";
V                   #eval "$velocity*sin(degToRad($alpha))";
flowVelocity        ($U $V 0);
pressure            0;
turbulentKE         0.24;
turbulentOmega      1.78;

