FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      extrudeMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//constructFrom mesh;
constructFrom patch;
sourceCase ".";
sourcePatches (back);
exposedPatchName front;

flipNormals true;
mergeFaces false;

extrudeModel plane;

nLayers 1;
expansionRatio 1.0;
thickness 1.0;

